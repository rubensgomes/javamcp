{
  "server": {
    "mode": "stdio",
    "host": "localhost",
    "port": 8000
  },
  "repositories": {
    "urls": [
      "https://github.com/apache/commons-lang.git",
      "https://github.com/google/guava.git"
    ],
    "local_base_path": "./repositories",
    "branch": "main"
  },
  "logging": {
    "level": "INFO",
    "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s",
    "output": "console",
    "file_path": null
  }
}
